package kidofuta;

public class java_practice7 {
	public static void main(String[]args) {
		int point = 59;

		if (60 <= point) {
			System.out.println("合格");
		}
		else {
			System.out.println("不合格");
		}

		if (80 <= point) {
			System.out.println("たいへんよくできました");
		}
		else if (60 <= point) {
			System.out.println("よくできました");
		}
		else {
			System.out.println("ざんねんでした");
		}


	}

}
