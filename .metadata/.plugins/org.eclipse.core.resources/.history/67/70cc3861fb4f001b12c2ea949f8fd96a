package kidofuta;

public class java_practice8 {
	public static void main(String[]args) {
		int inter = 50;
		int end = 60;

		if ( (inter >= 60) && (end >= 60)) {
			System.out.println("合格");
		}
		else if ((inter + end) >= 130) {
			System.out.println("合格");
		}
		else if ((inter + end) >= 100 && inter >= 90 || end >= 90) {
			System.out.println("合格");
		}
		else {
			System.out.println("不合格");
		}
	}

}
