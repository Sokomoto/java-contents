
public class Switch01 {
	public static void main(String[] args) {
		int val = 3;
		// switch文 :分岐処理の開始
		switch (val) {
		// case :値が3の場合にブロックを実行
		case 3:
			System.out.println("よくできました!");
			// break :switch文を終了する
			break;
			// case :値が2の場合にブロックを実行
		case 2:
			System.out.println("ふつうでした！");
			break;
		default:
			System.out.println("がんばろう！");
			break;
		}
	}

}
