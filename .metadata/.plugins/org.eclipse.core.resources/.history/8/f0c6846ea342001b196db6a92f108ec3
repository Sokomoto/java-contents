
public class Chapter05_02 {
	public static void main(String[] args) {
		long l = 100000000L;
		double d = 3.14;
		boolean b = true;
		System.out.println(l);
		System.out.println(d);
		System.out.println(b);

		Float f = 1.114f;
		Integer i = 7;
		Character c = '„ÅÇ';
		System.out.println(f);
		System.out.println(i);
		System.out.println(c);



	}

}
