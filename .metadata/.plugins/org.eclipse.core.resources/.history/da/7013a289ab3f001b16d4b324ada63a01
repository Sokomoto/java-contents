
public class Chapter05_1 {
	public static void main(String[] args) {
		long a1 = 10000000;
		double a2 = 1.1d;
		boolean a3 = true;
		System.out.println(a1);
		System.out.println(a2);
		System.out.println(a3);

		Float b1 = 0.0f;
		Integer b2 = 6;
		Character b3 = '„ÅÇ';
		System.out.println(b1);
		System.out.println(b2);
		System.out.println(b3);


	}

}
